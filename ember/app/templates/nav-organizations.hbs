<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">{{choosen}} <span class="caret"></span></a>
    <ul class="dropdown-menu" role="menu">
    	{{#each organization in organizations}}
    		<li class="dropdown-header">{{organization.name}}</li>
    		{{#each workspace in organization.workspaces}}
    		<li>{{#link-to "dashboard.organization.workspace.jobs" organization workspace}}{{workspace.name}}{{/link-to}}</li>
    		{{/each}}
    	{{/each}}
    </ul>
</li>