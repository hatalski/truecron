<div class="container fluid container-truecron">
    <br>
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-material-blue-grey-200">
                <div class="panel-heading truecron-panel-heading">
                    <h3 class="panel-title">My Profile</h3>
                </div>
                <div class="panel-body truecron-panel-body">
                    <div class="row">
                        <div class="col-md-2">
                            <h5>Email</h5>
                        </div>
                        <div class="col-md-10">
                            <div class="form-group-material-orange-700">
                                <div class="form-control-wrapper">
                                    {{#each userEmail in model.emails}}
                                        <span>{{userEmail.email}}</span>
                                    {{/each}}
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2">
                            <h5>Name</h5>
                        </div>
                        <div class="col-md-10">
                            <div class="form-group-material-orange-700">
                                <div class="form-control-wrapper">
                                    {{input id="userName" type="text" value=model.name disabled=true class="form-control empty" data-hint="Enter user name"}}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-material-blue-grey-200">
                <div class="panel-heading truecron-panel-heading">
                    <h3 class="panel-title">Password</h3>
                </div>
                <div class="panel-body truecron-panel-body">
                    <div class="row">
                        <div class="col-md-2">
                            <h5>Password</h5>
                        </div>
                        <div class="col-md-10">
                            <div class="form-group-material-orange-700">
                                <div class="form-control-wrapper">
                                    {{input id="password"
                                        type="password"
                                        value=model.password
                                        disabled=true
                                        class="form-control empty"
                                        required=true
                                        placeholder="Password (more than 8 characters in length)"
                                        pattern="(?=^.{8,}$)(?=.*\d)(?![.\n])(?=.*[A-Z])(?=.*[a-z]).*$"}}
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2">
                            <h5>Password Confirmation</h5>
                        </div>
                        <div class="col-md-10">
                            <div class="form-group-material-orange-700">
                                <div class="form-control-wrapper">
                                    <input id="repass" type="password" value="" class="form-control empty" data-hint="Confirm password">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-material-blue-grey-200">
                <div class="panel-heading truecron-panel-heading">
                    <h3 class="panel-title">Google Profile</h3>
                </div>
                <div class="panel-body truecron-panel-body">
                    <div class="row">
                        <div class="col-md-2">
                            <h5>Google Email</h5>
                        </div>
                        <div class="col-md-10">
                            <div class="form-group-material-orange-700">
                                <div class="form-control-wrapper">
                                    <input id="gmail" type="email" value="" class="form-control empty" data-hint="Enter Google Email">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
